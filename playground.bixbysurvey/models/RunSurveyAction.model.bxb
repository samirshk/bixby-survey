action (RunSurveyAction) {
  description (Run Survey Collect Ratings)
  collect {
    input (SurveyUser) {
      type (myself.Self)
      min (Optional)
    }
    input (SurveyNumber) {
      type (mycore.Integer)
      min (Required) max (One)
    }
    input (SurveyRating) {
      type (mycore.Text)
      min (Required) max (One)
    }
  }
  output (SurveyRatingText)
  type(Calculation)
}

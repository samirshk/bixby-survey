action (RunSurveyAction) {
  description (Run Survey Collect Ratings)
  collect {
    input (SurveyNumber) {
      type (SurveyNumberInteger)
      min (Required)
      max (One)
    }
    input (SurveyRating) {
      type (SurveyRatingText)
      min (Required) max (One)
    }
  }
  output (SurveyRunningRatingDecimal)
  type(Calculation)
}
